<link rel="stylesheet" href="jquery.mobile-1.4.5.min.css"/>
<script src="jquery-1.12.3.min.js"></script>
<script src="jquery.mobile-1.4.5.min.js"></script>

</head> 
<body>
<h1>Hello Food!</h1>
<!-- <p>Find me in app/views/foods/index.html.erb</p> -->



<table>


	<caption><b>Table of Foods</b></caption>
<tr>
			<th>Food Name</th>
			<th>Food Type</th>
			<th>Expiration Date</th>
			<th>Food Quality</th>
		</tr>	
	<% @foods.each do |food| %>
		<tr>
			<td><%= food.name %></td>
			<td><%= food.food_type %></td>
			<td><%= food.expiration %></td>
			<td><%= food.quality %></td>
		</tr>	

	<% end %>	
</table>

<a href='/foods/new'>Create new food item</a>
</body>
</html>